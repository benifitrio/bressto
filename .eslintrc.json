{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "linebreak-style":0,
        "no-underscore-dangle":0,
        "no-undef": 0,
        "no-mixed-operators":0,
        "radix":0,
        "consistent-return":0,
        "no-param-reassign":0,
        "import/no-extraneous-dependencies": 0,
        "no-alert":0,
        "no-plusplus":0
    }
}
